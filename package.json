    "react-addons-test-utils": "~15.4.2",
    "react-test-renderer": "~15.4.2",
    "rimraf": "~2.5.4",
    "style-loader": "~0.13.1",
    "stylus": "~0.54.5",
    "stylus-loader": "~3.0.2",
    "webpack": "~4.29.6",
    "webpack-cli": "~3.3.0",
    "webpack-dashboard": "~3.0.0",
    "webpack-dev-middleware": "~1.10.1",
    "webpack-dev-server": "~3.2.1",
    "webpack-hot-middleware": "~2.18.0"
  }
}
