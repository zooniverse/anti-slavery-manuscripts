{
  "name": "anti-slavery-manuscripts",
  "version": "1.0.0",
  "description": "Anti-Slavery Manuscripts invites volunteers to transcribe documents from the American abolitionist movement, working together with the Zooniverse community and the Boston Public Library.",
  "repository": {
    "type": "git",
    "url": "https://github.com/zooniverse/anti-slavery-manuscripts"
  },
  "keywords": [
    "zooniverse"
  ],
  "scripts": {
    "start": "BABEL_ENV=development check-engines && check-dependencies && webpack-dashboard -p 3001 -- webpack-dev-server --config ./webpack.config.js",
    "test": "echo 'This project has no tests.' || true",
    "test-travis": "true",
    "eslint": "eslint .",
    "build": "BABEL_ENV=production check-engines && check-dependencies && webpack --config webpack.production.config.js -p",
    "_stage": "BABEL_ENV=staging check-engines && check-dependencies && webpack --config webpack.config.js",
    "deploy-production": "NODE_ENV=production npm run build && publisssh dist zooniverse-static/www.antislaverymanuscripts.org",
    "deploy-staging": "NODE_ENV=staging npm run _stage && publisssh dist zooniverse-static/preview.zooniverse.org/antislaverymanuscripts"
  },
  "engines": {
    "node": "^10.15.3",
    "npm": ">=6"
  },
  "author": "Zooniverse",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zooniverse/anti-slavery-manuscripts"
  },
  "homepage": "https://www.antislaverymanuscripts.org/",
  "dependencies": {
    "counterpart": "~0.18.1",
    "graphql-request": "~1.3.4",
    "history": "~4.6.1",
    "markdownz": "~7.6.3",
    "panoptes-client": "^3.0.0-rc.0",
    "prop-types": "^15.5.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "~4.4.6",
    "react-rnd": "~5.1.0",
    "react-router": "~3.0.0",
    "react-select": "~1.0.0-rc.5",
    "redux": "~3.6.0",
    "redux-devtools-extension": "~1.0.0",
    "redux-logger": "~2.7.4",
    "redux-thunk": "~2.1.0",
    "zoo-grommet": "~0.3.0",
    "zooniverse-react-components": "github:zooniverse/Zooniverse-react-components#v0.9.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.0.0",
    "@babel/core": "~7.0.0",
    "@babel/node": "~7.2.2",
    "@babel/plugin-proposal-class-properties": "~7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "~7.3.2",
    "@babel/plugin-transform-object-assign": "~7.2.0",
    "@babel/plugin-transform-react-jsx": "~7.3.0",
    "@babel/preset-env": "~7.0.0",
    "@babel/preset-react": "~7.0.0",
    "@babel/register": "~7.0.0",
    "autoprefixer": "~6.5.3",
    "babel-eslint": "~7.1.1",
    "babel-loader": "~8.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.26.2",
    "chai": "~3.5.0",
    "check-dependencies": "~1.1.0",
    "check-engines": "~1.5.0",
    "css-loader": "~2.1.1",
    "enzyme": "~2.8.2",
    "eslint": "~4.18.2",
    "eslint-config-airbnb": "~13.0.0",
    "eslint-loader": "~1.6.1",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~2.2.3",
    "eslint-plugin-react": "~6.8.0",
    "eventsource-polyfill": "~0.9.6",
    "express": "~4.16.4",
    "extract-text-webpack-plugin": "~2.1.0",
    "file-loader": "~3.0.1",
    "html-webpack-plugin": "~3.2.0",
    "image-webpack-loader": "~4.6.0",
    "json-loader": "~0.5.4",
    "mini-css-extract-plugin": "~0.5.0",
    "mocha": "~4.0.0",
